{
  "name": "boombox",
  "version": "3.1.0",
  "description": "A free and open source Discord music bot built on Discord.js.",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json -w",
    "start": "node build/deploy.js && node build/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Boombox-Dev/Boombox.git"
  },
  "keywords": [
    "music",
    "discord",
    "bot",
    "youtube",
    "play"
  ],
  "author": "Boombox-Dev",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Boombox-Dev/Boombox/issues"
  },
  "homepage": "https://github.com/Boombox-Dev/Boombox#readme",
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@sapphire/framework": "^1.0.2",
    "@sentry/node": "^6.12.0",
    "@sentry/tracing": "^6.12.0",
    "async-redis": "^2.0.0",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.1.0",
    "erela.js": "^2.3.3",
    "erela.js-spotify": "^1.2.0",
    "humanize-duration": "^3.27.0",
    "node-redis-scan": "^1.3.3",
    "redis": "^3.1.2",
    "timestring": "^6.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^7",
    "@types/node": "^16.7.10",
    "@types/redis": "^2.8.32",
    "@types/timestring": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "prettier": "^2.3.2",
    "typescript": "^4.4.3"
  }
}
