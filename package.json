{
  "name": "Boombox",
  "version": "4.0.0",
  "description": "A free and open source discord music bot.",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json -w",
    "start": "yarn node build/src/index.js",
    "dev": "yarn concurrently \"yarn watch\" \"nodemon build/src/index.js\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "ci": "yarn run lint"
  },
  "main": "build/index.js",
  "repository": "https://github.com/Boombox-Discord/Boombox",
  "author": "Boombox-Discord",
  "license": "AGPL-3.0",
  "private": false,
  "dependencies": {
    "@sapphire/framework": "^3.0.0-next.f330d83.0",
    "@sapphire/plugin-logger": "^2.1.3",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "concurrently": "^7.0.0",
    "discord.js": "^13.6.0",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "eslint": "^7.12.1",
    "typescript": "^4.5.5"
  }
}
